 <turbo-frame id="second_frame">
    <div class="card shadow">
        <div class="card-header">
            Referensi Soal Efikasi
        </div>
        <div class="card-body">

            <ul class="list-group">
                <% @soal_efikasi.each_with_index do |efikasi, index| %>
                    <li class="list-group-item"><%= index +=1%>.<%= efikasi.deskripsi %></li>
                <% end %>
            </ul>
        
        </div>
    </div>
</turbo-frame>